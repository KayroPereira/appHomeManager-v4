<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DadosFirebaseActivity">

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/swOnOffChurrasqueiraGeral"
        android:layout_width="100dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:text="@string/onOff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvDFBInfo"
        android:layout_width="342dp"
        android:layout_height="598dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/vazio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swOnOffChurrasqueiraGeral" />

</androidx.constraintlayout.widget.ConstraintLayout>